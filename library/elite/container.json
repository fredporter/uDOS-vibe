{
  "$schema": "../container.schema.json",
  "container": {
    "id": "elite",
    "name": "TOYBOX Elite",
    "description": "Galaxy lens gameplay runtime adapter for uDOS TOYBOX (upstream Elite runtime via PTY).",
    "type": "git",
    "source": "https://github.com/fesh0r/newkind",
    "ref": "master",
    "cloned_at": null,
    "last_update": null,
    "commit": null,
    "version": "0.1.0-scaffold"
  },
  "policy": {
    "read_only": true,
    "auto_update": false,
    "update_channel": "wizard_only",
    "verify_signatures": false
  },
  "integration": {
    "wrapper_path": "extensions/play/toybox/elite",
    "service_path": "wizard/services/toybox/elite_adapter.py",
    "handler_path": "core/commands/gameplay_handler.py",
    "install_script": "library/elite/setup.sh",
    "layers": "600-799"
  },
  "metadata": {
    "license": "GPL-2.0",
    "homepage": "https://github.com/fesh0r/newkind",
    "documentation": "docs/specs/uDOS-Gameplay-Anchors-v1.3-Spec.md",
    "maintainer": "uDOS Wizard Server",
    "category": "toybox-gameplay"
  },
  "toybox": {
    "profile": "elite",
    "lens": "galaxy",
    "upstream_policy": "no-fork",
    "reskin_policy": "udos-ui-redraw-2026"
  },
  "service": {
    "port": 7422,
    "health_check_url": "http://localhost:7422/health",
    "browser_route": "/ui/elite"
  },
  "launch_config": {
    "command": ["python3", "-m", "wizard.services.toybox.elite_adapter"],
    "cwd": "."
  },
  "repo_path": "memory/library/containers/elite"
}
