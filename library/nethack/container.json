{
  "$schema": "../container.schema.json",
  "container": {
    "id": "nethack",
    "name": "NetHack",
    "description": "Classic roguelike dungeon crawler with ASCII graphics",
    "type": "git",
    "source": "https://github.com/NetHack/NetHack",
    "ref": "NetHack-3.6.7_Released",
    "cloned_at": null,
    "last_update": null,
    "commit": null,
    "version": "3.6.7"
  },
  "policy": {
    "read_only": true,
    "auto_update": false,
    "update_channel": "wizard_only",
    "verify_signatures": false
  },
  "integration": {
    "wrapper_path": "extensions/play/nethack/",
    "service_path": null,
    "handler_path": "core/commands/gameplay_handler.py",
    "install_script": "extensions/setup/install_nethack.sh",
    "layers": "400-455"
  },
  "metadata": {
    "license": "NGPL",
    "homepage": "https://www.nethack.org/",
    "documentation": "extensions/play/nethack/README.md",
    "maintainer": "uDOS Wizard Server",
    "category": "games"
  },
  "build": {
    "type": "make",
    "configure": "./sys/unix/setup.sh",
    "build_command": "make all",
    "install_prefix": "/opt/nethack",
    "tce_dependencies": [
      "ncurses",
      "ncurses-dev",
      "compiletc"
    ]
  },
  "repo_path": "memory/library/containers/nethack"
}