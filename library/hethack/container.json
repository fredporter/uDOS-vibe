{
  "$schema": "../container.schema.json",
  "container": {
    "id": "hethack",
    "name": "TOYBOX Hethack",
    "description": "Dungeon lens gameplay runtime adapter for uDOS TOYBOX (upstream NetHack via PTY).",
    "type": "git",
    "source": "https://github.com/NetHack/NetHack",
    "ref": "NetHack-3.6.7_Released",
    "cloned_at": "2026-02-17T12:11:39.195071Z",
    "last_update": null,
    "commit": null,
    "version": "0.1.0-scaffold"
  },
  "policy": {
    "read_only": true,
    "auto_update": false,
    "update_channel": "wizard_only",
    "verify_signatures": false
  },
  "integration": {
    "wrapper_path": "extensions/play/toybox/hethack",
    "service_path": "wizard/services/toybox/hethack_adapter.py",
    "handler_path": "core/commands/gameplay_handler.py",
    "install_script": "library/hethack/setup.sh",
    "layers": "400-499"
  },
  "metadata": {
    "license": "NGPL",
    "homepage": "https://www.nethack.org/",
    "documentation": "docs/specs/uDOS-Gameplay-Anchors-v1.3-Spec.md",
    "maintainer": "uDOS Wizard Server",
    "category": "toybox-gameplay"
  },
  "toybox": {
    "profile": "hethack",
    "lens": "dungeon",
    "upstream_policy": "no-fork",
    "reskin_policy": "udos-ui-redraw-2026"
  },
  "service": {
    "port": 7421,
    "health_check_url": "http://localhost:7421/health",
    "browser_route": "/ui/hethack"
  },
  "launch_config": {
    "command": [
      "python3",
      "-m",
      "wizard.services.toybox.hethack_adapter"
    ],
    "cwd": "."
  },
  "repo_path": "memory/library/containers/hethack"
}